{
	"name": "util.keys",
	"version": "0.0.22",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Maintains a set of unique keys generated for a react component",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.keys"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.keys/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"prettier": "@jmquigley/prettier-config",
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"util.constants": "^0.0.30",
		"util.string": "^0.0.36",
		"util.toolbox": "^0.0.86"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@babel/plugin-transform-modules-commonjs": "^7.7.5",
		"@babel/plugin-transform-modules-umd": "^7.7.4",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-typescript": "^7.7.4",
		"@babel/runtime": "^7.7.6",
		"@jmquigley/prettier-config": "^1.0.0",
		"@types/jest": "^24.0.23",
		"@types/node": "^12.12.17",
		"babel-loader": "^8.0.6",
		"babel-minify": "^0.5.1",
		"babel-preset-minify": "^0.5.1",
		"coveralls": "^3.0.9",
		"debug": "^4.1.1",
		"jest": "^24.9.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.7.3",
		"util.pkgwrap": "^0.0.115",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"pkgwrap": {
		"cleanup": [
			"docs/",
			"tsconfig.tsbuildinfo"
		]
	},
	"scripts": {
		"all": "yarn install && yarn build && yarn docs && yarn test && yarn manifest",
		"build": "yarn lint && yarn prettier && yarn build:umd && yarn build:es && yarn build:cjs",
		"build:cjs": "babel index.ts --source-type module --out-file index.cjs.min.js",
		"build:es": "minify index.js --sourceType module --out-file index.es.min.js",
		"build:umd": "pkgwrap --build --webpack",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"docs": "pkgwrap --docs",
		"lint": "pkgwrap --lint",
		"manifest": "npm pack --dry-run",
		"prettier": "pkgwrap --prettier",
		"test": "jest"
	}
}
